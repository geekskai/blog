{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "start": "next dev",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "size:check": "opennextjs-cloudflare build && wrangler deploy --outdir bundled/ --dry-run",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@json-table/core": "^0.1.0",
    "@mesmotronic/next-adsense": "^1.0.0",
    "@next/bundle-analyzer": "14.2.3",
    "@opennextjs/cloudflare": "latest",
    "@shaggytools/nhtsa-api-wrapper": "^3.0.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/cheerio": "^0.22.35",
    "@types/dom-to-image": "^2.6.0",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "cheerio": "^1.1.0",
    "contentlayer2": "0.5.1",
    "date-fns": "^4.1.0",
    "dom-to-image": "^2.6.0",
    "dompurify": "^3.2.6",
    "esbuild": "0.20.2",
    "file-saver": "^2.0.5",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "heic2any": "^0.0.4",
    "html2canvas-pro": "^1.5.13",
    "image-size": "1.0.0",
    "jsdom": "^26.1.0",
    "jspdf": "^3.0.4",
    "jszip": "^3.10.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.525.0",
    "mdast-util-to-hast": "^13.2.0",
    "next": "14.2.35",
    "next-contentlayer2": "0.5.1",
    "next-intl": "^4.3.9",
    "next-themes": "^0.3.0",
    "pdfjs-dist": "^5.3.93",
    "pliny": "0.2.1",
    "postcss": "^8.4.24",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "react-fast-marquee": "^1.6.5",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-share": "5.1.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "remarkable": "^2.0.1",
    "soundcloud-downloader": "^1.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.3",
    "turndown": "^7.2.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@types/mdx": "^2.0.12",
    "@types/react": "^18.2.73",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sharp": "^0.34.5",
    "typescript": "^5.1.3",
    "wrangler": "latest"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
